    agent any
    
    environment{
    path= "/opt/maven/apache-maven-3.6.3/bin:$PATH"
    }
        stages{
            stage("git checkout"){
                
            
            steps{
                git ''
            }    
            }
            stage("maven build"){
                steps{
                sh "mvn clean package"
            }
            
            
            }
        }
    
}
